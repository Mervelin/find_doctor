<table summary="Visit form fields">
  <tr>
    <th>Office</th>
    <td><%= f.collection_select(:office_id, @visit.doctor.offices, :id, :name) %></td>
  </tr>
  <tr>
    <th>Start time</th>
    <td><%= f.datetime_select(:start_time, minute_step: 15) %></td>
  </tr>
  <tr>
    <th>End time</th>
    <td><%= f.datetime_select(:end_time, minute_step: 15) %></td>
  </tr>
</table>
