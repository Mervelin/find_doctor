<% if @doctors %>
  <table class="table table-hover" summary="Page list">
    <tr class="header">
      <th>First name</th>
      <th>Last name</th>
      <th></th>
      <!--<th>Visible</th>-->
      <!--<th>Permlink</th>-->
      <!--<th>Sections</th>-->
      <!--<th>Actions</th>-->
    </tr>

    <% @doctors.each do |doctor| %>

        <tr>
          <td><%= doctor.first_name %></td>
          <td><%= doctor.last_name %></td>
          <td><%= link_to('Show profile', welcome_doctor_path(id: doctor.id ), class: 'btn
          btn-primary')
          %></td>
          <!--<td></td>-->
          <!--<td></td>-->
        </tr>
    <% end %>
  </table>
<% else %>
 <p>Nothing found, try again with different input</p>
<% end %>